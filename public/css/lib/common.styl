.zWrapLg
    margin 0 auto
    width 1200px
.zWrap
    margin 0 auto
    width 1000px
.bgPrimary
    background #0e90d2
a
    color inherit
    text-decoration none
.zCode
    border 1px solid #dedede
    background #f5f2f0
    word-break break-all
    word-wrap break-word
    white-space pre-wrap
    font-size 13px
    color #555
    padding 0.5em 1.2em
    line-height 1.4em
    pre 
        margin 0
        padding 0

.zHref
    color #0e90d2
    &:hover
        color #10a0ea
        
.borderRight
    border-right 1px solid #ddd

.tRed
    color #c7254e
.tMuted
    color #888
.bgGray
    background-color #BABFCD

