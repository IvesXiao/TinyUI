@import './params'


/* originally authored by Nick Pettit - https://github.com/nickpettit/glide */

@keyframes fadeInDown 
    0% 
        opacity 0
        transform translate3d(0, -40px, 0)
    100% 
        opacity 1
        transform none
        
@keyframes fadeInUp
    0% 
        opacity 0
        transform translate3d(0, -5px, 0)
    100% 
        opacity 1
        transform 0

@keyframes spin
    0% 
        transform rotate(0deg)
    100%
        transform rotate(360deg)

@keyframes spinPrimary
    0% 
        transform rotate(0deg)
    50% 
        color #0c80ba
    100% 
        transform rotate(360deg)

@keyframes spinInfo
    0% 
        transform rotate(0deg)
    50% 
        color #46b8da
    100% 
        transform rotate(360deg)

@keyframes spinSuccess
    0% 
        transform rotate(0deg)
    50% 
        color #4db14d
    100% 
        transform rotate(360deg)

@keyframes spinWarning
    0% 
        transform rotate(0deg)
    50% 
        color #d93c37
    100% 
        transform rotate(360deg)
        
@keyframes spinDanger
    0% 
        transform rotate(0deg)
    50% 
        color #d93c37
    100% 
        transform rotate(360deg)
        
// 进度条动画
@keyframes progressStriped
    0% 
        background-position 0 
    100% 
        background-position 36px
        
// 从下方淡入
.zFadeInDown
    animation-name fadeInDown
    animation-duration .15s

// 从上方淡入
.zFadeInUp
    animation-name fadeInUp
    animation-duration .15s

// 旋转动画
.zSpin:before
    animation spin 0.8s linear infinite normal
    display inline-block
.zSpinPrimary:before
    animation spinPrimary 0.8s linear infinite normal
    display inline-block
.zSpinInfo:before
    animation spinInfo 0.8s linear infinite normal
    display inline-block
.zSpinSuccess:before
    animation spinSuccess 0.8s linear infinite normal
    display inline-block
.zSpinWarning:before
    animation spinWarning 0.8s linear infinite normal
    display inline-block
.zSpinDanger:before
    animation spinDanger 0.8s linear infinite normal
    display inline-block
    
//Bounce

@keyframes bounce 
    0%, 20%, 53%, 80%, 100% 
        animation-timing-function cubic-bezier(0.215, 0.610, 0.355, 1.000)
        transform translate3d(0,0,0)
    40%, 43% 
        animation-timing-function cubic-bezier(0.755, 0.050, 0.855, 0.060)
        transform translate3d(0, -30px, 0)
    70% 
        animation-timing-function cubic-bezier(0.755, 0.050, 0.855, 0.060)
        transform translate3d(0, -15px, 0)
    90%
        transform translate3d(0,-4px,0)
.zBounce
    animation-name bounce
    transform-origin center bottom
    animation-duration 1s

@keyframes flash 
    0%, 50%, 100% 
        opacity 1
    25%, 75% 
        opacity 0
.zFlash
  animation-name flash
  animation-duration 1s

@keyframes pulse 
    0% 
        transform scale3d(1, 1, 1)
    50% 
        transform scale3d(1.25, 1.25, 1.25)
    100%
        transform scale3d(1, 1, 1)
.zPulse 
    animation-name pulse
    animation-duration 1s

@keyframes rubberBand 
    0% 
        transform scale3d(1, 1, 1)
    30%
        transform scale3d(1.25, 0.75, 1)
    40% 
        transform scale3d(0.75, 1.25, 1)
    50% 
        transform scale3d(1.15, 0.85, 1)
    65% 
        transform scale3d(.95, 1.05, 1)
    75% 
        transform scale3d(1.05, .95, 1)
    100% 
        transform scale3d(1, 1, 1)
.zRubberBand
    animation-name rubberBand
    animation-duration 1s

@keyframes shake 
    0%, 100% 
        transform translate3d(0, 0, 0)
    10%, 30%, 50%, 70%, 90% 
        transform translate3d(-10px, 0, 0)
    20%, 40%, 60%, 80% 
        transform translate3d(10px, 0, 0)
.zShake 
  animation-name shake
  animation-duration 1s
  

@keyframes hinge 
    0% 
        transform-origin top left
        animation-timing-function ease-in-out
    20%, 60% 
        transform rotate3d(0, 0, 1, 80deg)
        transform-origin top left
        animation-timing-function ease-in-out
    40%, 80% 
        transform rotate3d(0, 0, 1, 60deg)
        transform-origin top left
        animation-timing-function: ease-in-out
        opacity 1
    100% 
        transform translate3d(0, 700px, 0)
        opacity 0
.zHinge 
    animation-name hinge
    animation-duration 1s

@keyframes rollIn 
    0% 
        opacity 0
        transform translate3d(-100%, 0, 0) rotate3d(0, 0, 1, -120deg)
    100% 
        opacity 1
        transform none
.zRollIn
    animation-name rollIn
    animation-duration 1s

@keyframes rollOut 
    0%
        opacity 1
    100% 
        opacity 0
        transform translate3d(100%, 0, 0) rotate3d(0, 0, 1, 120deg)
.zRollOut 
    animation-name rollOut
    animation-duration 1s

  
@keyframes swing 
    20% 
        transform rotate3d(0, 0, 1, 15deg)
    40% 
        transform rotate3d(0, 0, 1, -10deg)
    60% 
        transform rotate3d(0, 0, 1, 5deg)
    80%     
        transform rotate3d(0, 0, 1, -5deg)
    100% 
        transform rotate3d(0, 0, 1, 0deg)
.zSwing
